; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="akai_template"
config/tags=PackedStringArray("template")
run/main_scene="res://Stages/Testing/MainTest.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

FullscreenControl="*res://addons/fullscreen_control/FullscreenControl.gd"

[dotnet]

project/assembly_name="akaicore_template"

[editor_plugins]

enabled=PackedStringArray("res://addons/QuickPluginManager/plugin.cfg", "res://addons/SignalVisualizer/plugin.cfg", "res://addons/criss-cross/plugin.cfg", "res://addons/dictionary_inspector/plugin.cfg", "res://addons/fullscreen_control/plugin.cfg", "res://addons/imjp94.yafsm/plugin.cfg", "res://addons/kanban_tasks/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/qodot/plugin.cfg")

[physics]

3d/physics_engine="JoltPhysics3D"

[rendering]

lights_and_shadows/directional_shadow/soft_shadow_filter_quality=4
lights_and_shadows/positional_shadow/soft_shadow_filter_quality=4
environment/ssao/quality=3
scaling_3d/mode=1
scaling_3d/scale=1.15
textures/decals/filter=5
textures/light_projectors/filter=5
anti_aliasing/quality/msaa_3d=3
anti_aliasing/quality/screen_space_aa=1
lights_and_shadows/positional_shadow/atlas_quadrant_0_subdiv=3
lights_and_shadows/positional_shadow/atlas_quadrant_1_subdiv=3
lights_and_shadows/positional_shadow/atlas_quadrant_2_subdiv=4
lights_and_shadows/positional_shadow/atlas_quadrant_3_subdiv=5
environment/defaults/default_environment="res://Settings/DefaultEnv.tres"
